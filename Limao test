-- Players.miguelalt97.PlayerGui.ScreenGui.gui.scripts.TextButton.LocalScript
local function C_9()
	local script = G2L["LocalScript_9"];
	-- Pega refer√™ncias importantes	
	local Button = script.Parent	
	local Player = game.Players.LocalPlayer	
	local Character = Player.Character or Player.CharacterAdded:Wait()	
	local Humanoid = Character:WaitForChild("Humanoid")	
	
	-- Vari√°vel para controlar se o pulo infinito est√° ativo ou n√£o	
	local isJumping = false	
	local jumpConnection = nil	
	
	-- Fun√ß√£o que faz o personagem pular	
	local function JumpInfinitely()	
	    -- Enquanto 'isJumping' for verdadeiro e o Humanoid existir	
	    while isJumping and Humanoid do	
	        -- A√ß√£o de pulo (for√ßa o pulo)	
	        Humanoid.Jump = true	
	        	
	        -- Espera um pequeno per√≠odo antes de tentar pular novamente.	
	        -- Ajuste este valor se o pulo estiver muito r√°pido/lento.	
	        task.wait(0.1) 	
	    end	
	end	
	
	-- Fun√ß√£o que √© chamada quando o bot√£o √© clicado	
	local function onButtonClicked()	
	    -- Inverte o estado atual	
	    isJumping = not isJumping	
	
	    if isJumping then	
	        print("Pulo infinito ativado.")	
	        -- Inicia a rotina de pulo em uma nova 'thread'	
	        jumpConnection = task.spawn(JumpInfinitely)	
	    else	
	        print("Pulo infinito desativado.")	
	        -- N√£o precisamos parar explicitamente a task.spawn, 	
	        -- pois o 'while' loop em JumpInfinitely vai parar 	
	        -- na pr√≥xima itera√ß√£o quando 'isJumping' for false.	
	    end	
	end	
	
	-- Conecta a fun√ß√£o ao evento de clique do bot√£o	
	Button.MouseButton1Click:Connect(onButtonClicked)	
	
	-- Garante que o estado seja desativado se o jogador morrer	
	Player.CharacterAdded:Connect(function(newCharacter)	
	    Character = newCharacter	
	    Humanoid = Character:WaitForChild("Humanoid")	
	    isJumping = false -- Desativa o pulo ao renascer	
	end)	
	
end;
task.spawn(C_9);

return G2L["ScreenGui_1"], require;
