-- Players.miguelalt97.PlayerGui.ScreenGui.gui.scripts.TextButton.LocalScript
local function C_22()
	local script = G2L["LocalScript_22"];
	-- Script (dentro do seu bot√£o no StarterGui)	
	local Button = script.Parent	
	local ReplicatedStorage = game:GetService("ReplicatedStorage")	
	local Workspace = game:GetService("Workspace")	
	
	local EVENT_NAME = "ChangeGravityEvent"	
	local NOVA_GRAVIDADE = 1	
	local GRAVIDADE_PADRAO = 196.2	
	
	local isGravityLow = false	
	
	-- --- L√ìGICA DO SERVIDOR ---	
	-- Esta parte ser√° executada pelo servidor	
	local function setupServer()	
	    -- 1. Tenta encontrar ou cria o RemoteEvent no ReplicatedStorage	
	    local ChangeGravityEvent = ReplicatedStorage:FindFirstChild(EVENT_NAME)	
	    if not ChangeGravityEvent then	
	        ChangeGravityEvent = Instance.new("RemoteEvent")	
	        ChangeGravityEvent.Name = EVENT_NAME	
	        ChangeGravityEvent.Parent = ReplicatedStorage	
	        print("Servidor: RemoteEvent '" .. EVENT_NAME .. "' criado em ReplicatedStorage.")	
	    end	
	
	    -- 2. Define a fun√ß√£o do servidor para mudar a gravidade	
	    local function onChangeGravityRequest(player, targetGravity)	
	        -- Verifica se o valor √© v√°lido antes de aplicar (seguran√ßa)	
	        if typeof(targetGravity) == "number" and targetGravity >= 0 and targetGravity <= 200 then	
	            Workspace.Gravity = targetGravity	
	            print("Servidor: Gravidade mudada para " .. targetGravity .. " por " .. player.Name)	
	        else	
	            warn("Servidor: Tentativa inv√°lida de mudar gravidade.")	
	        end	
	    end	
	
	    -- Conecta a fun√ß√£o ao evento	
	    ChangeGravityEvent.OnServerEvent:Connect(onChangeGravityRequest)	
	end	
	
	-- --- L√ìGICA DO CLIENTE ---	
	-- Esta parte ser√° executada pelo cliente	
	local function setupClient()	
	    -- 1. Espera pelo RemoteEvent que o servidor criou (ou j√° existe)	
	    local ChangeGravityEvent = ReplicatedStorage:WaitForChild(EVENT_NAME)	
	    	
	    -- 2. Configura o texto do bot√£o (Se houver)	
	    if Button:IsA("TextButton") then	
	        Button.Text = "Ativar Gravidade Baixa"	
	    end	
	    	
	    -- 3. Define a fun√ß√£o de clique	
	    local function onButtonClicked()	
	        isGravityLow = not isGravityLow	
	        	
	        local targetGravity	
	        if isGravityLow then	
	            targetGravity = NOVA_GRAVIDADE	
	            if Button:IsA("TextButton") then	
	                Button.Text = "Desativar Gravidade"	
	            end	
	        else	
	            targetGravity = GRAVIDADE_PADRAO	
	            if Button:IsA("TextButton") then	
	                Button.Text = "Ativar Gravidade Baixa"	
	            end	
	        end	
	        	
	        -- Envia a nova gravidade para o servidor	
	        ChangeGravityEvent:FireServer(targetGravity)	
	        print("Cliente: Solicitado mudan√ßa para gravidade " .. targetGravity)	
	    end	
	
	    -- 4. Conecta a fun√ß√£o ao evento de clique	
	    Button.MouseButton1Click:Connect(onButtonClicked)	
	end	
	
	-- --- INICIALIZA√á√ÉO ---	
	
	-- Verifica se este script est√° rodando no servidor ou no cliente	
	if game:GetService("RunService"):IsServer() then	
	    -- Se for o servidor, configura o RemoteEvent e a fun√ß√£o de mudan√ßa de gravidade	
	    setupServer()	
	else	
	    -- Se for o cliente, configura o bot√£o	
	    setupClient()	
	end	
	
end;
task.spawn(C_22);

return G2L["ScreenGui_1"], require;
